<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="2.0dip" />
            <gradient android:startColor="#00ffffff" android:endColor="#00ffffff" android:angle="270.0" android:centerColor="#00ffffff" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="2.0dip" />
                <gradient android:startColor="@color/lite_colorAccent" android:endColor="@color/lite_colorAccent" android:angle="0.0" android:centerColor="@color/lite_colorAccent" />
            </shape>
        </clip>
    </item>
</layer-list>